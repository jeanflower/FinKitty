{
  "name": "finkitty",
  "homepage": "https://jeanflower.github.io/FinKitty",
  "version": "1.4.71",
  "engines": {
    "node": "16.x"
  },
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.0.2",
    "@types/chart.js": "^2.9.35",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^16.14.21",
    "@types/react-data-grid": "^4.0.8",
    "@types/react-dom": "16.9.4",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.1.2",
    "crypto-js": "^4.1.1",
    "dateformat": "^5.0.3",
    "dotenv-cli": "^7.2.1",
    "file-saver": "^2.0.5",
    "gh-pages": "^4.0.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.3",
    "react": "^16.14.0",
    "react-bootstrap": "^2.7.0",
    "react-chartjs-2": "^5.2.0",
    "react-data-grid": "^6.1.0",
    "react-dom": "^16.14.0",
    "react-spacer": "^1.3.0",
    "ws": "^8.4.2"
  },
  "scripts": {
    "postinstall": "react-scripts build",
    "start": "export HTTPS=true&&PORT=3000 react-scripts start",
    "startLocal": "export HTTPS=true&&PORT=3000&&REACT_APP_SERVER_URL_NOT_SECRET=http://localhost:3001/finkitty/ react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "predeploy": "npm run build",
    "deploy": "NODE_DEBUG=gh-pages&&REACT_APP_SERVER_URL_NOT_SECRET=https://desolate-refuge-81883.herokuapp.com/finkitty/ gh-pages -d build",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "prefer-rest-params": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.2.5",
    "@types/react-tooltip": "^4.2.4",
    "@types/selenium-webdriver": "^4.0.16",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chromedriver": "^113.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.0",
    "husky": "^8.0.3",
    "jest-watch-typeahead": "^2.2.1",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.1",
    "selenium-webdriver": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/tests/**"
    ]
  }
}
